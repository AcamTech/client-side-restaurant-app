{
  "name": "react-slingshot",
  "version": "3.0.1",
  "description": "Starter kit for creating apps with React and Redux",
  "scripts": {
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist lint:tools",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw src/.",
    "lint:watch": "npm run lint -- --watch",
    "lint:tools": "eslint webpack.config.js tools",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "build:js": "babel-node tools/build.js",
    "prebuild": "npm run clean-dist && npm run build:html",
    "build": "babel-node tools/build.js && npm run open:dist",
    "test": "cross-env NODE_ENV=test mocha --reporter progress --compilers js:babel-core/register --recursive \"./src/**/*.spec.js\" --require ignore-styles",
    "test:watch": "npm run test -- --watch",
    "deploy": "./node_modules/surge/lib/cli.js --project ./dist --domain toque-app.surge.sh"
  },
  "author": "Cory House",
  "license": "MIT",
  "dependencies": {
    "alertify.js": "1.0.12",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "c3": "0.4.18",
    "connect-history-api-fallback": "1.4.0",
    "create-react-class": "15.6.2",
    "date-fns": "1.29.0",
    "dom-easy": "1.1.1",
    "firebase": "4.6.1",
    "lodash.merge": "4.6.0",
    "magnific-popup": "1.1.0",
    "normalizr": "2.1.0",
    "object-assign": "4.1.1",
    "oui-dom-events": "0.2.2",
    "pad-left": "2.1.0",
    "prop-types": "15.6.0",
    "ramda": "0.25.0",
    "rc-select": "7.1.0",
    "rd3": "0.8.1",
    "re-base": "3.0.5",
    "react": "16.0.0",
    "react-body-classname": "1.2.0",
    "react-dom": "16.0.0",
    "react-hot-loader": "3.1.1",
    "react-modal": "3.1.2",
    "react-redux": "5.0.6",
    "react-router": "3.2.0",
    "react-router-redux": "4.0.8",
    "react-select": "1.0.0-rc.10",
    "react-tabs": "2.1.0",
    "redux": "3.7.2",
    "redux-form": "5.3.6",
    "redux-promise-middleware": "4.4.2",
    "redux-thunk": "2.2.0",
    "victory": "0.24.0"
  },
  "devDependencies": {
    "autoprefixer": "6.3.6",
    "babel-cli": "6.10.1",
    "babel-core": "6.9.1",
    "babel-eslint": "6.0.4",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.5.0",
    "browser-sync": "2.13.0",
    "chai": "3.5.0",
    "cheerio": "0.20.0",
    "colors": "1.1.2",
    "copy-webpack-plugin": "3.0.1",
    "cross-env": "1.0.8",
    "css-loader": "0.23.1",
    "eslint": "2.12.0",
    "eslint-import-resolver-webpack": "0.3.0",
    "eslint-loader": "1.3.0",
    "eslint-plugin-babel": "3.2.0",
    "eslint-plugin-import": "1.8.1",
    "eslint-plugin-promise": "1.3.2",
    "eslint-plugin-react": "5.1.1",
    "eslint-watch": "2.1.11",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "ignore-styles": "4.0.0",
    "mocha": "2.5.3",
    "node-sass": "3.7.0",
    "npm-run-all": "2.1.2",
    "postcss-loader": "0.9.1",
    "redux-logger": "2.6.1",
    "rimraf": "2.5.2",
    "sass-loader": "3.2.0",
    "string-replace-loader": "1.0.2",
    "style-loader": "0.13.1",
    "surge": "0.18.0",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0",
    "yargs": "4.7.1"
  },
  "keywords:": [
    "react",
    "reactjs",
    "react-router",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "redux",
    "flux",
    "boilerplate",
    "starter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/coryhouse/react-slingshot"
  }
}
